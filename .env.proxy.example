# .env.proxy.example
#
# Copy to .env and fill in your values:
#   cp .env.proxy.example .env

# Shared HMAC secret between agent and keyring proxy (generate a strong random value)
KEYRING_PROXY_SECRET=change-me-to-a-strong-random-secret

# Password for the encrypted-file keystore inside the proxy container
KEYSTORE_PASSWORD=change-me-to-a-strong-password

# Ports (optional â€” defaults shown)
KEYRING_PROXY_PORT=3100
