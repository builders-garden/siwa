{
  "name": "erc-8004-test",
  "private": true,
  "type": "module",
  "scripts": {
    "server": "tsx server/index.ts",
    "agent": "tsx agent/cli.ts",
    "agent:create": "tsx agent/cli.ts create-wallet",
    "agent:register": "tsx agent/cli.ts register",
    "agent:signin": "tsx agent/cli.ts sign-in",
    "agent:flow": "tsx agent/cli.ts full-flow",
    "agent:status": "tsx agent/cli.ts status",
    "agent:test-keystore": "tsx agent/cli.ts test-keystore",
    "reset": "bash scripts/reset.sh",
    "proxy": "tsx proxy/index.ts",
    "proxy:test": "KEYSTORE_BACKEND=proxy tsx agent/cli.ts test-proxy",
    "dev": "concurrently \"pnpm run server\" \"sleep 2 && pnpm run agent:flow\"",
    "dev:proxy": "concurrently \"pnpm run proxy\" \"pnpm run server\" \"sleep 3 && KEYSTORE_BACKEND=proxy pnpm run agent:flow\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "concurrently": "^9.0.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.14.3",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.0",
    "tsx": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "typescript": "^5.5.0"
  }
}
